$(function(){var e=0;$(".toggle-brushstroke").click(function(){$(".toggle-brushstroke.focused").removeClass("focused"),src=$(this).data("src"),$("#brushstroke-img-src").attr("src",src),$(this).addClass("focused"),e=$(this).data("index"),$.ajax({url:host+"/API/call/track",type:"POST",data:{id:$(this).attr("id")}})}),$("body").keydown(function(t){switch(t.keyCode){case 37:t.preventDefault(),tmpIndex=e-1,$(".toggle-brushstroke[data-index="+tmpIndex+"]").length>0&&(e--,$(".toggle-brushstroke[data-index="+e+"]").click());break;case 38:t.preventDefault();break;case 39:t.preventDefault(),tmpIndex=e+1,$(".toggle-brushstroke[data-index="+tmpIndex+"]").length>0&&(e++,$(".toggle-brushstroke[data-index="+e+"]").click());break;case 40:t.preventDefault()}}),mansorize()});var mansorize=function(){var e=$(".mansorize-grid");e.imagesLoaded(function(){e.masonry({itemSelector:".mansorize-elem",percentPosition:!0,columnWidth:".mansorize-elem"})})};