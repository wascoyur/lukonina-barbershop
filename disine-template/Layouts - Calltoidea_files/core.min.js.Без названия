function close_popup_menu_listener(){$(".wrap_main_content_global").click(function(){!0===mobile_menu_open&&($("#lateral_popup").css({width:"0%"}),mobile_menu_open=!1)}),$(".wrap_lateral_content").click(function(e){e.stopPropagation()})}function closeToast(){$("#toast").css({visibility:"hidden",bottom:"0px",opacity:"0"})}$(function(){modalListener(),$("#subscribe_submit").click(function(){email=$("#subscribe_email").val(),isMail(email)?$.ajax({url:host+"/API/user/subscribe",type:"POST",data:{email:email},success:function(){$("#footer_subscribe").slideDown("500",function(){$("#footer_subscribe").remove()}),toastit("Thank you for the interest in Calltoidea. We'll contact you as soon as possible.","success",5e3)}}):toastit("Insert a valid email address","error",3e3)})}),document.addEventListener("touchstart",function(){},!0);var mobile_menu_open=!1;$("#open_menu_header").click(function(){$("#lateral_popup").css({width:"90%"}),setTimeout(function(){close_popup_menu_listener(),mobile_menu_open=!0},300)});var submenu_listener=function(){$(".wrap-submenu-header-web").length>0&&(scroll_top=$(".wrap-submenu-header-web").offset().top,$(document).on("scroll",function(){$(document).scrollTop()>scroll_top?$(".wrap-submenu-header-web").addClass("fixed"):$(".wrap-submenu-header-web").removeClass("fixed")})),$("#open-all-works").length>0&&($("#open-all-works").click(function(e){e.stopPropagation(),$("#submenu-projects").addClass("show"),$("#title-submenu").addClass("hide")}),$("#submenu-projects").click(function(e){e.preventDefault(),e.stopPropagation()}),$("#submenu-projects").children("li").children("a").click(function(){return window.location=$(this).attr("href"),!1}),$("body").click(function(){$("#submenu-projects").removeClass("show"),$("#title-submenu").removeClass("hide")}))},toastit=function(e,o,t){"success"==(o=void 0!==o?o:"default")&&(o="ok"),t=void 0!==t?t:5e3,(e=e.trim()).length>0&&($("#toast").children(".alert").removeClass("error ok default"),$("#toast").children(".alert").addClass(o),$("#toast").children(".alert").children("p").html(e),$("#toast").css("visibility","visible"),$("#toast").animate({bottom:"20px",opacity:"1"},350),t&&setTimeout(function(){closeToast()},t))};$(function(){$("#close-toast").click(function(){closeToast()})});var tabsListener=function(){[].slice.call(document.querySelectorAll(".tabs")).forEach(function(e){new CBPFWTabs(e)})},modalListener=function(){function e(){document.body.style.overflow="visible",$(".main_modal").each(function(){$(this).fadeOut(100)})}$("[data-toggle=modal]").click(function(){id_modal=$(this).data("modal"),$("#"+id_modal).css("display","table"),document.body.style.overflow="hidden"}),$(".close-modal").click(function(){e()}),$(".wrap_modal").click(function(){e()}),$(".main_popup, .main_preview_picture").click(function(e){e.stopPropagation()}),$(document).keyup(function(o){k=o.keyCode,27==k&&e()})};